{
    "name": "diegovernan/laravel-calendar",
    "description": "Google Calendar package for Laravel",
    "authors": [
        {
            "name": "Diego Vernan",
            "email": "diegovernan@gmail.com",
            "homepage": "https://www.diegovernan.com.br/"
        }
    ],
    "version": "1.0.0",
    "license": "MIT",
    "require": {
      "php": "^7.4 || ^8.0",
      "google/apiclient": "^2.4",
      "illuminate/auth": "~5.8|^6.0",
      "illuminate/config": "~5.8|^6.0",
      "illuminate/database": "~5.8|^6.0",
      "illuminate/routing": "~5.8|^6.0",
      "illuminate/session": "~5.8|^6.0",
      "illuminate/support": "~5.8|^6.0",
      "swiftmailer/swiftmailer": "~5.8|^6.0"
    },
    "autoload": {
      "psr-4": {
        "DiegoVernan\\LaravelCalendar\\": "src/"
      }
    },
    "extra": {
      "laravel": {
        "providers": [
          "DiegoVernan\\LaravelCalendar\\LaravelCalendarServiceProvider"
        ],
        "aliases": {
          "LaravelCalendar": "DiegoVernan\\LaravelCalendar\\Facade\\LaravelCalendar"
        }
      }
    }
}
