{
    "name": "diegovernan/laravel-calendar",
    "description": "Google Calendar package for Laravel",
    "authors": [
        {
            "name": "Diego Vernan",
            "email": "diegovernan@gmail.com",
            "homepage": "https://www.diegovernan.com.br/"
        }
    ],
    "version": "v1.0.0",
    "license": "MIT",
    "require": {
        "php": "^7.4 || ^8.0",
        "google/apiclient": "^2.15.0",
        "illuminate/auth": "~7.0|^8.0",
        "illuminate/config": "~7.0|^8.0",
        "illuminate/database": "~7.0|^8.0",
        "illuminate/routing": "~7.0|^8.0",
        "illuminate/session": "~7.0|^8.0",
        "illuminate/support": "~7.0|^8.0",
        "swiftmailer/swiftmailer": "~7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "DiegoVernan\\LaravelCalendar\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DiegoVernan\\LaravelCalendar\\LaravelCalendarServiceProvider"
            ],
            "aliases": {
                "LaravelCalendar": "DiegoVernan\\LaravelCalendar\\Facade\\LaravelCalendar"
            }
        }
    }
}